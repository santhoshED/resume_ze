				<p class="heading">SOME PRIOR IMPLEMENTATION EXPERIENCE:</p>
				<hr class="underline_3">
				<div id="project" class="project">
					
					<div class="project_block">
					<div id="project_block_top">
						<div>
							<p class="project_name">Project Name:</p>
							<p class="project_name_1"><%=prior_projects.name[i]%></p>
						</div>
					
						<div class="side-heading">
							<p>Description:</p>
						</div>
						<div class="project_content">
							<% var prior_projects_description = prior_projects.description[i].split("\r\n");
							for(var j=0;j<prior_projects_description.length;j++){
								if(prior_projects_description[j].length>0){%>
							<p><%=prior_projects_description[j]%></p><%}}%>
						</div>
						<% if(prior_projects.approach[i]){%>
						<div class="side-heading">
							<p>Approach:</p>
						</div>
						<div class="project_content">
							<% var prior_projects_description = prior_projects.approach[i].split("\r\n");
							for(var j=0;j<prior_projects_description.length;j++){
								if(prior_projects_description[j].length>0){%>
							<p><%=prior_projects_description[j]%></p><%}}%>
						</div>
						<%}if(prior_proj_images[i]){%>
					</div>
						<div id="project_block_bottom">
						<div id="project_im">
						<div class="side-heading">
							<p>Technologies:</p>
