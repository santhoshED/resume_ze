								<%for(var i=0;i<front_end_images.length;i++){%>
								<img class="skill_image" src=<%="./images/full_stack/"+front_end_images[i]%>></img>
								<%}%>
							</div>
							<%} if(databases_images.length>0){%>
							<div class="skill_heading">
								<p>DataBases:</p>
							</div>
							<div class="skill_images">
								<%for(var i=0;i<databases_images.length;i++){%>
								<img class="skill_image" src=<%="./images/full_stack/"+databases_images[i]%>></img>
								<%}%>
							</div>
							<%} if(technologies_images.length>0){%>
							<div class="skill_heading">
								<p>Architectures & Cloud Services:</p>
							</div>
							<div class="skill_images">
								<%for(var i=0;i<technologies_images.length;i++){%>
								<img class="skill_image" src=<%="./images/full_stack/"+technologies_images[i]%>></img>
								<%}%>
							</div>
							<%}%>
						</div>
					</div>
				</div>
			</div>
		</div>
