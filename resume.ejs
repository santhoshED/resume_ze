<!Doctype html>
<html>
<head>
	<title>RESUME</title>
	<link rel="stylesheet" type="text/css" href="./css/resume.css">
	
</head>
<body>
	<div class="wrapper">
		<div class="indexpage">
			<div class="header">
				<img class="logo" src="./images/logo.png"/>
				<a class="url">www.zemosolabs.com</a>
			</div>
			<div class="body">			
				<div class ="introduction">
					<p class="employee_name"><%= employee_name %></p>
					<p class="employee_position"><%=employee_position%></p>
					<p class="employee_position" style="display:inline-block">Location:</p>
					<p class="employee_location"><%=employee_location%></p>
				</div>
				<div class="res_row">
					<div class="res_col">
						<p class="heading">SUMMARY</p>
						<hr class="underline">
						<div class="summary">
							<%for(var i=0;i<summary.length;i++){
								if(summary[i].length>0){%>
							<p class="summary_content"><%=summary[i]%></p><%}}%>
						</div> 
					</div>
					<div class="res_col_2">
						<p class="heading">EDUCATION</p>
						<hr class="underline">
						<div id="education" class="education">
						<% if(college.name.constructor===Array){ 
						for(var i=0;i<college.name.length;i++){
							if(college.name[i].length>0){%>

							<div class="education_block">	
								<div>
									<img class="education_img" src=<%="./images/university/"+college_images[i]%>></img>
								</div>
								<div class="education_content">
									<p class="education_name"><%=college.name[i]%></p>
									<p class="education_course"><%=college.degree[i]%></p>
									<p class="education_time"><%=college.year[i]%></p>
								</div>
							</div>
							<%}}}
							else{%>
							<div class="education_block">	
								<div>
									<img class="education_img" src=<%="./images/university/"+college_images[0]%>></img>
								</div>
								<div class="education_content">
									<p class="education_name"><%=college.name%></p>
									<p class="education_course"><%=college.degree%></p>
									<p class="education_time"><%=college.year%></p>
								</div>
							</div>
							<%}%>
						</div>
					</div>
				</div>
				<div class="res_row">
					<div class="res_col">
						<p class="heading">WORK EXPERIENCE</p>
						<hr class="underline">
						<div id="work_experience"class="work-experience">
						<% if(workex.designation.constructor === Array){ 
						for(var i=0;i<workex.designation.length;i++){
							if(workex.designation[i].length>0){%>
							<div class="work_experience_block">
								<div class="work_experience_vector">
								<%
								var img_1;
								var img_2;
								if(i == 0 ){img_1="./images/vector/arrow.png"} 
									else {img_1="./images/vector/small.png"}
								if(i==workex.designation.length-1|| workex.designation[i+1].length ==0){img_2="./images/vector/start.png"}
									else{img_2="./images/vector/large.png"}
								%>
									<img class="vector-image" src=<%=img_1%>></img>
									<img class="vector-image_1" src="./images/vector/circle.png"/>
									<img class="vector-image" src="./images/vector/small.png"/>
									<img class="vector-image" src=<%=img_2%>></img>
								</div>
								<div class="work_experience_content">
									<p class="work_ex_designation"><%=workex.designation[i]%></p>
									<p class="work_ex_time">(<%=workex.from[i]%> to <%=workex.to[i]%>)</p>
									<p class="work_ex_organisation"><%=workex.organisation[i]%></p>
									<p class="work_ex_place"><%=workex.place[i]%></p>
								</div>
							</div>
							<%}}}
							else{%>
							<div class="work_experience_block">
								<div class="work_experience_vector">
									<img class="vector-image" src="./images/vector/arrow.png"/>
									<img class="vector-image_1" src="./images/vector/circle.png"/>
									<img class="vector-image" src="./images/vector/small.png"/>
									<img class="vector-image" src="./images/vector/start.png"/>
								</div>
								<div class="work_experience_content">
									<p class="work_ex_designation"><%=workex.designation%></p>
									<p class="work_ex_time">(<%=workex.from%> to <%=workex.to%>)</p>
									<p class="work_ex_organisation"><%=workex.organisation%></p>
									<p class="work_ex_place"><%=workex.place%></p>
								</div>
							</div>
							<%}%>	
						</div>
					</div>
					<div class="res_col_2">
						<p class="heading">SKILLS</p>
						<hr class="underline">
						<div id="skills" class ="skills">
							<% if(server_side_images.length>0){%>
							<div class="skill_heading">
								<p>Server Side:</p>
							</div>
							<div class="skill_images">
								<%for(var i=0;i<server_side_images.length;i++){%>
								<img class="skill_image" src=<%="./images/full_stack/"+server_side_images[i]%>></img>
								<%}%>
							</div>
							<%} if(front_end_images.length>0){%>
							<div class="skill_heading">
								<p>Front End:</p>
							</div>
