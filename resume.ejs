								if(projects_description[j].length>0){%>
							<p><%=projects_description[j]%></p><%}}%>
						</div>
						</div>
					<div id="project_block_bottom">
						<div id="project_im">
						<div class="side-heading">
							<p>Technologies:</p>
						</div>
						
						<div class="project_content project_images">
							<% for(var j=0;j<proj_images[0].length;j++){%>

							<img class="project_image" src=<%="./images/full_stack/"+proj_images[0][j]%>></img>

							<%}%>
						</div>
					
						<div class="side-heading">
							<p>Contribution:</p>
						</div>
						<div class="project_content">
							<% var projects_description = projects.contribution.split("\r\n");
							for(var j=0;j<projects_description.length;j++){
								if(projects_description[j].length>0){%>
							<p><%=projects_description[j]%></p><%}}%>
						</div>
						<div class="side-heading">
							<p>Duration:</p>
						</div>
						<div class="project_content">
							<p><%=projects.duration%></p>
						</div>
						</div>
						</div>	
					</div>
					
				</div>
			</div>
		</div>
			<%}}%>
			<%if(prior_projects.name.length>0){
			if(prior_projects.name.constructor===Array){ 
						for(var i=0;i<prior_projects.name.length;i++){
							if(prior_projects.name[i].length>0){%>
