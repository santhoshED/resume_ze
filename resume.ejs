								if(projects_description[j].length>0){%>
							<p><%=projects_description[j]%></p><%}}%>
						</div>
						<div class="side-heading">
							<p>Duration:</p>
						</div>
						<div class="project_content">
							<p><%=projects.duration[i]%></p>
						</div>
					</div>
					</div>	
							
					</div>
				</div>
			</div>
		</div>
					<%}}}
					else{%>
