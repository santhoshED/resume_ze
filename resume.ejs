						</div>
						<%}if(prior_projects.contribution[i]){%>
						<div class="side-heading">
							<p>Contribution:</p>
						</div>
						<div class="project_content">
							<% var prior_projects_description = prior_projects.contribution[i].split("\r\n");
							for(var j=0;j<prior_projects_description.length;j++){
								if(prior_projects_description[j].length>0){%>
							<p><%=prior_projects_description[j]%></p><%}}%>
						</div>
						<%}if(prior_projects.duration[i]){%>
						<div class="side-heading">
							<p>Duration:</p>
						</div>
						<div class="project_content">
							<p><%=prior_projects.duration[i]%></p>
						</div>
							<%}%>
						</div>
					</div>
					</div>
				</div>
			</div>
		</div>
